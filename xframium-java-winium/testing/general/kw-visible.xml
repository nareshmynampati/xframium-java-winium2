<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
  <model>
    
    <page name="home" />

  </model>

  <function name="kw-visible" active="true">
    
    <step type="EXISTS" name="aOpen" page="home"  />
    <step type="EXISTS" name="deleteButton" page="home"  />
    
    <step type="VISIBLE" name="deleteButton" page="home" inverse="true" />
    <step type="CLICK" name="aOpen" page="home" wait="1000" />
    <step type="VISIBLE" name="deleteButton" page="home" />
    <step type="CLICK" name="aOpen" page="home" wait="1000" />
    <step type="VISIBLE" name="deleteButton" page="home" inverse="true" />


  </function>

</suite>
