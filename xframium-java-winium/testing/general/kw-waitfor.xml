<?xml version="1.0" encoding="UTF-8"?>
<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
  <model>
    
    <page name="home" />

  </model>

  <function name="kw-waitfor" active="true">
    
    <step type="WAIT_FOR" name="aOpen" page="home"  />
    <step type="WAIT_FOR" name="aOpen" page="home"  >
      <parameter type="STATIC" value="5" />
    </step>
    <step type="EXISTS" name="deleteButton" page="home"  />
    
    <step type="VISIBLE" name="deleteButton" page="home" inverse="true" />
    <step type="WAIT_FOR" name="aOpen.VISIBLE" page="home" />
    <step type="WAIT_FOR" name="aOpen.CLICKABLE" page="home" />
    <step type="CLICK" name="aOpen" page="home" />
    <step type="WAIT_FOR" name="deleteButton.VISIBLE" page="home" />
    <step type="WAIT_FOR" name="deleteButton.CLICKABLE" page="home" />
    <step type="VISIBLE" name="deleteButton" page="home" />


  </function>

</suite>
